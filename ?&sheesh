-- JailBreak
elseif game.PlaceId == 606849621 then
    local JBwin = Flux:Window("MAMBA HUB", "Jailbreak", Color3.fromRGB(0, 140, 0), Enum.KeyCode.RightShift)
    local JBMain = JBwin:Tab("Main", "http://www.roblox.com/asset/?id=6023426915")
